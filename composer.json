{
    "name": "flowsa/craft-ohdear-health-check",
    "description": "A Craft CMS plugin that checks the health of your site using the Oh Dear API.",
    "type": "craft-plugin",
    "keywords": [ "craft", "craftcms", "plugin", "ohdear", "health", "check"],
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Flow Communications",
            "homepage": "https://www.flowsa.com"
        }
    ],
    "support": {
        "email": "info@flowsa.com",
        "issues": "https://github.com/flowsa/ohdear-health-check/issues",
        "source": "https://github.com/flowsa/ohdear-health-check",
        "docs": "https://github.com/flowsa/ohdear-health-check"
      },
    "require": {
        "craftcms/cms": "^5.3.0",
        "ohdearapp/health-check-results": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ohdearhealthcheck\\ohdearhealthcheck\\": "src/"
        }
    },
    "extra": {
        "name": "Oh Dear Health Checker",
        "handle": "ohdear-health-check",
        "developer": "Flow Communications",
        "developerUrl": "https://github.com/flowsa",
        "class": "ohdearhealthcheck\\ohdearhealthcheck\\OhDearHealthCheck"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
